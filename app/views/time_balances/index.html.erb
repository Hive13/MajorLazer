<h1>Listing time_balances</h1>

<table>
  <tr>
    <th>User</th>
    <th>Minutes</th>
    <th>Submitted by</th>
    <th>Transaction time</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @time_balances.each do |time_balance| %>
  <tr>
    <td><%= time_balance.user_id %></td>
    <td><%= time_balance.minutes %></td>
    <td><%= time_balance.submitted_by_id %></td>
    <td><%= time_balance.transaction_time %></td>
    <td><%= link_to 'Show', time_balance %></td>
    <td><%= link_to 'Edit', edit_time_balance_path(time_balance) %></td>
    <td><%= link_to 'Destroy', time_balance, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Time balance', new_time_balance_path %>
